
<% include ./elem/header.ejs %>


<!-- Page Content -->
 <div class="container">

   <!-- Page Header -->
   <div class="row">
     <div class="col-lg-12">
       <h1 class="page-header">Downloads à GOGO
                   <small>Unlimited torrents download</small>
               </h1>
     </div>
   </div>
   <!-- /.row -->

   <!-- Content Row -->
   <div class="row">



     <% if (api1.length != 0) {
         for (var i=0 ; i < api1.movies.length ; i++) {
         var movie = api1.movies[i]; %>
   <div class="col-md-3">
     <div class="card hoverable">
       <div class="card-image">
         <img src="<%= movie.medium_cover_image %>">
         <span class="card-title"> <%= movie.title %> </span>
       </div>
       <div class="card-content">
         <p> <%= movie.year %> </p>
         <% for (var j=0 ; j < movie.torrents.length ; j++) {
            var torrent = movie.torrents[j]; %>
            <b> <a href=" <%= torrent.url %>"><%= torrent.quality %> </a>  - peers: <%= torrent.peers %> - seeds: <%= torrent.seeds %> </b> <br/>
         <% } %>
       </div>
       <div class="card-action">
         <a href="http://mdbootstrap.com/product/magic-portfolio-for-video-maker/">
           <button type="button" class="btn btn-info waves-effect waves-light">Read more</button>
         </a>
       </div>
     </div>
   </div>
     <% } %>
   <% } else { %>
         <p>
           Aucun films n'est présent dans cette catégorie
         </p>
     <% }  %>
</div>

<% include ./elem/footer.ejs %>
